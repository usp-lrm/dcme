net: "./models/segnet_3_train.prototxt"
test_initialization: false
# test_iter: 200
# test_interval: 200000

base_lr: 1e-8
lr_policy: "step"
gamma: 0.1
stepsize:  60000
max_iter:  80000

display: 50
momentum: 0.9
# iter_size: 2
weight_decay: 0.0005
snapshot: 2000
snapshot_prefix: "./weights/segnet_3"
solver_mode: GPU
