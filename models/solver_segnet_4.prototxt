net: "./models/segnet_4_train.prototxt"
test_initialization: false
# test_iter: 200
# test_interval: 200000

base_lr: 1e-8
lr_policy: "step"
stepsize: 100000
max_iter: 100000
gamma: 0.1

display: 50
momentum: 0.9
iter_size: 5
weight_decay: 0.0005
snapshot: 2000
snapshot_prefix: "./weights/segnet_4"
solver_mode: GPU
